{
  "name": "radio-player",
  "version": "0.1.0",
  "description": "Web app Radio Player. The web app that gives the ability to listen Internet radio.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "css:min": "gulp",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "build:dev:dll": "webpack --colors --progress --config webpack.dev.dll.js",
    "build:dev:dll-jsx": "webpack --colors --progress --config webpack.dev.dll-jsx.js",
    "build:dll": "webpack --colors --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --colors --progress --config webpack.dll-jsx.js"
  },
  "keywords": [
    "radio-player"
  ],
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "del": "^5.1.0",
    "eslint": "^6.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^5.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jspm": "^0.16.55",
    "terser-webpack-plugin": "^2.2.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "jspm": {
    "dependencies": {
      "@babel/runtime": "npm:@babel/runtime@^7.7.6",
      "howler": "npm:howler@^2.1.2",
      "react": "npm:react@^16.12.0",
      "react-dom": "npm:react-dom@^16.12.0",
      "react-redux": "npm:react-redux@^7.1.3",
      "redux": "npm:redux@^4.0.4"
    },
    "devDependencies": {}
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
