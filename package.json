{
  "name": "radio-player",
  "version": "0.3.0",
  "description": "Web app Radio Player. The web app that gives the ability to listen Internet radio.",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config webpack.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "css:min": "node min-css.js",
