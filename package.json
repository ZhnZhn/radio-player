  },
  "dependencies": {
    "@babel/runtime": "^7.28.6",
    "howler": "^2.2.4",
    "preact": "^10.28.3",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.29.0",
    "@babel/eslint-parser": "^7.28.6",
    "@babel/plugin-transform-react-jsx": "^7.28.6",
    "@babel/plugin-transform-runtime": "^7.29.0",
