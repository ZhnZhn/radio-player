{
  "name": "radio-player",
  "version": "0.2.0",
  "description": "Web app Radio Player. The web app that gives the ability to listen Internet radio.",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "css:min": "gulp",
    "lint:hooks": "eslint -c .eslintrc-hooks.js ./src > lint-hooks.txt",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "build:dll": "webpack --colors --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --colors --progress --config webpack.dll-jsx.js"
  },
  "keywords": [
    "radio-player"
  ],
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhnZhn/radio-player.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "del": "^6.0.0",
    "eslint": "^7.12.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^5.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "html-webpack-plugin": "^4.5.0",
    "react-refresh": "^0.9.0",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.3.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },  
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "howler": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5"
  }
}
